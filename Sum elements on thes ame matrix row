import java.math.*;
public class SommaElementiRigaMatrice {

	public static void main(String[] args) {
		int[][]M;
		int riga;
        int sommariga= Somma(M,riga);
        System.out.println(sommariga);
	}
public static int Somma(int[][]M,int riga){
	int sommaelementi= 0;
	for(int j=0;j<M.length;j++)
		sommaelementi=sommaelementi + M[riga][j];
	return sommaelementi;
	
}
}
