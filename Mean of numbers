import java.util.Scanner;
import java.math.*;
public class medinumeri {
	/* fare un programma che calcoli la media degli elementi di un vettore */
	public static void main(String[] args) {	 
double []V={3,3,3,3,3};
double mediaelementi= CalcolaMedia(V);

System.out.println("la media aritmetica degli elementi Ã¨ pari ad : " + mediaelementi);
}

public static double CalcolaMedia (double []V){

	double somma = 0;
	for (int i=0;i<V.length;i++)
		somma=somma+V[i];
	double media= somma/V.length;
	return media;
	
}
}

