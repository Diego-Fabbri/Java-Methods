public class Stampamatrice {

	public static void main(String[] args) {
		int [][] x ={{4,6,8},
		             {4,5,6},
		             {4,4,5}};
		stampaMatrice(x);
	}

public static void stampaMatrice(int[][] x){
for(int i=0;i<x.length;i++){

	for(int j=0;j<x[0].length;j++)
        System.out.print(x[i][j]+" ");
	System.out.println();

	}
}
}
